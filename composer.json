{
    "name": "chriswoodford/propel-cli",
    "type": "library",
    "homepage": "http://ideapatterns.ca/",
    "autoload": {
        "psr-0": {
            "PropelCli": "lib/"
        }
    },
    "bin": ["bin/propel-cli", "bin/propel-cli.php"],
    "require": {
        "php": ">= 5.3.2",
        "propel/propel_runtime": "1.3.0",
        "doctrine/dbal": "2.2.x",
        "symfony/console": "2.2.x"  
    },
    "require-dev": {
        "jaz303/phake": "dev-master@dev",
        "phpunit/phpunit": "3.7.x"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.propelorm.org",
            "vendor-alias": "propel"
        }
    ]
}
